#include<stdio.h>

int main(){
    //formular for area of a rectangle = a*b
    // int a = 4;
    // int b = 7;
    int a;
    int b;
    printf("Enter the value of length\n");
    scanf("%d", &a);

    printf("Enter the value of breadth\n");
    scanf("%d", &b);
    printf("The area of rectangle is %d", a*b);
    return 0;
}
